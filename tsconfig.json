{
  "compilerOptions": {
    "noImplicitAny": false,             
    "noUnusedLocals": false,           
    "noUnusedParameters": false,     
    "skipLibCheck": true,              
    "strict": false,                  
    "allowJs": true,                  
    "ignoreDeprecations": "5.0",        
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@/sanity/*": ["./sanity/*"],
      "@/api/*": ["/api/*"],
      "@/lib/*": ["./src/lib/*"],
      "@/config/*": ["./src/config/*"],
      "@/utils/*": ["./src/utils/*"],
      "@/components/*": ["./src/components/*"],
      "@/images/*": ["./src/images/*"],
      // "@/sanityLibrary/*": ["./src/sanity/*"],
      // "@/env/*": [".env/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/login/oldpage.jsx", "src/models/Notification.js", "src/UserContext.js", "src/components/navigationMenu/navigation.js", "src/components/FollowMessageButtons.js"],
  "exclude": ["node_modules"]
}
